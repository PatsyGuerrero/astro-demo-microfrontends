---
import BaseLayout from '../layouts/BaseLayout.astro';
import ContentPage from '../components/ContentPage.astro';
import Art from '../components/Art.jsx';
import Hero from '../components/Hero.jsx';
import data from '../json/Art.json'
import NavBar from '../components/NavBar.jsx';
import Cocktails from '../components/Cocktails.jsx';
import About from '../components/About.jsx'
import Menu from '../components/Menu.jsx'
import Contact from '../components/Contact.jsx'
const allContent = Object.values(import.meta.glob('./subpages/*.md', { eager: true }));
const pageTitle = "Pagina de contenido";

---

<>
	<NavBar client:only="react" data={data.nav} />
	<Hero client:only="react" data={data.hero}/>
	<Cocktails client:only="react" data={data.cockails} />
	<About client:only="react" data={data.about} />
	<Art client:only="react" data={data.art}/>
	<Menu client:only="react"  data={data.menu}/>
	<Contact client:only="react" data={data.contact} />
</>